{
  "name": "video-chat-frontend",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run generate:sitemap && npm run generate:rss && tsc && vite build",
    "build:prerender": "npm run generate:sitemap && npm run generate:rss && tsc && vite build && PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true npm run snap",
    "snap": "react-snap",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src",
    "generate:sitemap": "tsx scripts/generate-sitemap.ts",
    "generate:rss": "tsx scripts/generate-rss.ts"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@sentry/react": "^10.29.0",
    "axios": "^1.6.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "flowbite": "^3.1.2",
    "flowbite-react": "^0.12.9",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.400.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.30.1",
    "socket.io-client": "^4.7.0",
    "sonner": "^1.4.0",
    "uuid": "^9.0.1",
    "web-vitals": "^5.1.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^20.19.23",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.1.0",
    "react-snap": "^1.23.0",
    "sitemap": "^8.0.1",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.44.0",
    "tsx": "^4.20.6",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.0.0"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/",
      "/terms-of-service",
      "/privacy-policy",
      "/cookie-policy",
      "/blog",
      "/blog/omegle-alternatives-2024-best-random-video-chat-sites",
      "/blog/video-chat-safety-tips-protect-yourself-online",
      "/blog/make-friends-online-random-video-chat-guide",
      "/blog/random-video-chat-vs-dating-apps-which-better",
      "/blog/learn-languages-random-video-chat-native-speakers",
      "/blog/random-video-chat-for-introverts-social-anxiety-tips"
    ]
  }
}
